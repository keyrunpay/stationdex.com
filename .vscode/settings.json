{
  "prettier.enable": false,
  "eslint.enable": true,
  "cSpell.words": [
    "noopener",
    "noreferrer"
  ],
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  
  "[scss]":{
    "editor.formatOnSave": true,
    "prettier.enable": true,
  }
}
